{
    "Description": "macro_rebase",
    "Spec": "The 'rebase' macro should rebase the link relative to the ToBaseDir to one relative to the ToFile.",
    "InputFilePath": "Inputs/macro_rebase.input.json",
    "AnswerFilePath": "Answers/macro_rebase.answer.json",
    "Config": {
        "Parameters": {
            "FromBaseDirPath": "From/",
            "FromFileRelPath": "x/y/z/macro_rebase.md",
            "ToBaseDirPath": "To/",
            "ToFileRelPath": "x/y/z/macro_rebase.html",
            "RebaseOtherRelativeLinks": false,
            "ExtensionMap": {
                ".md": ".html"
            }
        }
    }
}